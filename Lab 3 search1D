import java.util.Scanner;
class search1d
{public static void main(String args[])
{	
	Scanner sc= new Scanner(System.in);
	System.out.println("enter the no. to be searched out of these elements: 3,5,8,3,42,7,3,8,7");
	int num,i,j;
	int a[]={3,5,8,3,42,7,3,8,7};
	num=sc.nextInt();
	System.out.println("The no.is present at:");
	for(i=0;i<9;i++)
	{
		if(a[i]==num)
			System.out.print("a["+i+"]");
	}
}
}
